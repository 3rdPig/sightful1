                <input type="text" class="form_control3" id="input1" placeholder="" name="undergrad_school" value="<%= @user.undergrad_school%>">
              </div>
              <div class="col-sm-6">
                <label for="inputEmail3" class="control-label">Major</label>
                <input type="text" class="form_control3" id="input2" placeholder="" name="undergrad_major" value="<%= @user.grad_major%>">
              </div>
            </div>
            <div class="form-group">
              <div class="buttons_div">
                <button type="submit" class="updatebtn5">Update</button>
              </div>
            </div>
        <% end %>
        <!--</form> -->
      </div>
    </div>
  </div>
</section>

