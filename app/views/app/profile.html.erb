        <%= form_tag '/updateProfile' , class:'form-horizontal mypro_form' , method:'POST' ,id:'example-form' ,remote:true do %>
            <!-- <form class="form-horizontal mypro_form" id="example-form" name="my-form"> -->
            <div class="form-group">
              <div class="col-sm-6">
                <label for="inputEmail3" class="control-label">First Name</label>
                <input type="text" class="form_control3" name="fname" id="input1" placeholder="Jun" value="<%= @user.first_name %>">
              </div>
              <div class="col-sm-6">
                <label for="inputEmail3" class="control-label">Last Name</label>
