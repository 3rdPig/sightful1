              <div class="col-sm-6">
                <label for="inputEmail3" class="control-label">Major</label>
                <input type="text" class="form_control3" id="input2" placeholder="" name="grad_major" value="<%= @user.grad_major%>">
              </div>
            </div>
            <div class="form-group">
